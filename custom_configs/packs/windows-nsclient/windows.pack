{
    "name":"windows-nsclient",
    "description":"Standard windows checks, like CPU, RAM and disk space. Checks are done using NSClient++",
    "path":"os/",
    "macros":{
    "_NSCLIENTPASSWORD": {"type":"string",
        "description":"The NSClient++ password"
    },
    "_NSCLIENT_PORT": {"type":"integer",
        "description":"The NSClient++ agent listen port"
    },
    "_NSCLIENT_VERSION": {"type":"string",
        "description": "NSClient++ standard version, default: NSClient++ 0.4.3.143 2015-04-29"
    },    
    "_LOAD1_THRESHOLD": {"type":"string",
        "description": "Load1 warning and critical thresholds, default: 1,80,90"
    },
    "_LOAD5_THRESHOLD": {"type":"string",
        "description": "Load5 warning and critical thresholds, default: 5,80,90"
    },
    "_LOAD15_THRESHOLD": {"type":"string",
        "description": "Load15 warning and critical thresholds, default: 15,80,90"
    },
    "_MEMORY_WARN": {"type":"doublepercent",
       "description": "Physical memory and swap warning level"
    },
    "_MEMORY_CRIT": {"type":"doublepercent",
       "description": "Physical memory and swap critical level"     
    },

